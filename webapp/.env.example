// Example environment configuration
// Copy this file to .env in the webapp root directory and update the values

// Backend API Configuration
VITE_API_URL=http://localhost:5500/api/v0

// Firebase Configuration (optional - for chat and notifications)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef

// OAuth Configuration (optional)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_APPLE_CLIENT_ID=your_apple_client_id_here

// Environment
NODE_ENV=development
